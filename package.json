    "mdsvex": "^0.9.8",
    "prettier": "^2.5.0",
    "prettier-plugin-svelte": "^2.5.0",
    "svelte": "^3.44.2",
    "svelte-highlight": "^5.1.3",
    "svelte-preprocess": "^4.9.8",
    "svelte2tsx": "^0.4.11"
  },
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vpanyushenko/bubbles.git"
